()\n\n        for (var i = 0; i < already.length; i++){\n            var name = already[i].data.name;\n            var id = already[i].id;\n            var value = false;\n            if (name == topic) {\n                name = topic;\n                id = id;\n                value = true;\n                break;\n            } else {\n                console.log(\"False\")\n                value = false;\n            }\n        }\n\n        if (!value) {\n            db.collection(\"blog\").add({\n                name: topic\n            })\n            .then(function (docRef) {\n                addNewDocument(docRef.id);\n            })\n            .catch(function(error) {\n                console.error(\"Error adding document: \", error);\n            });\n\n            \n        } else {\n            db.collection(\"blog\").doc(id).collection(\"messages\").add({\n                name: user,\n                title: title,\n                ideas: ideas,\n                timestamp:firebase.firestore.FieldValue.serverTimestamp(),\n            })\n        }\n\n        history.push('/')\n\n    }\n\n\n\n    return ( \n        <div className=\"addNewPost\">\n            \n            <div className=\"login__container\">\n                <h2>Write something amazing</h2>\n                <form>\n                    <h5>Name</h5>\n                    <input\n                        type=\"text\"\n                        value={user}\n                        onChange={e => setUser(e.target.value)}\n                    />\n                    <h5>On Which Topic you want to write</h5>\n                    <input\n                        type=\"text\"\n                        value={topic}\n                        onChange={e => setTopic(e.target.value)}\n                    />\n                    <h5>Title</h5>\n                    <input\n                        type=\"text\"\n                        value={title}\n                        onChange={e => setTitle(e.target.value)}\n                    />\n                    <h5>Content</h5>\n                    <div className=\"content\">\n                        <textarea\n                            name={ideas}\n                            onChange={e => setIdeas(e.target.value)}\n\n                        ></textarea>\n                    </div>\n                    <button\n                        className=\"login__signInButton\"\n                        type=\"submit\"\n                        onClick={newPost}\n                    >Post</button>\n                </form>\n            </div>\n        </div>\n    )\n}\n\nexport default AddNewPost\n","/Users/maharshgheewala/Desktop/apps/Blog-App/blog/src/Compoents/Body/Body.js",[],"/Users/maharshgheewala/Desktop/apps/Blog-App/blog/src/features/userSlice.js",[],"/Users/maharshgheewala/Desktop/apps/Blog-App/blog/src/firebase.js",[],"/Users/maharshgheewala/Desktop/apps/Blog-App/blog/src/Compoents/Body/Post/Post.js",["111","112","113"],"/Users/maharshgheewala/Desktop/apps/Blog-App/blog/src/Compoents/Body/Topics/Topic.js",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":10,"column":13,"nodeType":"120","messageId":"121","endLine":10,"endColumn":19},{"ruleId":"118","severity":1,"message":"122","line":1,"column":17,"nodeType":"120","messageId":"121","endLine":1,"endColumn":26},{"ruleId":"118","severity":1,"message":"123","line":2,"column":8,"nodeType":"120","messageId":"121","endLine":2,"endColumn":10},{"ruleId":"118","severity":1,"message":"124","line":7,"column":12,"nodeType":"120","messageId":"121","endLine":7,"endColumn":20},{"ruleId":"118","severity":1,"message":"125","line":7,"column":22,"nodeType":"120","messageId":"121","endLine":7,"endColumn":33},{"ruleId":"118","severity":1,"message":"126","line":5,"column":8,"nodeType":"120","messageId":"121","endLine":5,"endColumn":14},{"ruleId":"127","severity":1,"message":"128","line":45,"column":22,"nodeType":"1